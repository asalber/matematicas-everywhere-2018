	booktitle = "{Proceedings of the Workshop on Ontology Matching (OM2007) at ISWC/ASWC2007, Busan, South Korea}",
	editor = "Shvaiko, Pavel and Euzenat, J{\'e}r{\^o}me and Giunchiglia, Fausto and He, Bin",
	interhash = "6fed7dca8701b39a871459508906a9cf",
	intrahash = "eaa9403bceff9f6733f5015d9bce519e",
	keywords = "no-leido ontologies ontology-alignment",
	month = nov,
	timestamp = "2012-06-07T11:07:57.000+0200",
