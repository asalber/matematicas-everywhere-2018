@article{tversky1977features,
	abstract = "Se hace un an{\'a}lisis de las principales caracter{\'i}sticas de las medidas de similitud. En particular se muestra que no tienen porqu{\'e} ser sim{\'e}tricas. Se plantea un modelo (conocido como el modelo de constraste de Tversky) que pretende capturar las caracter{\'i}sticas de las funciones de similitud o distancias. Seg{\'u}n el modelo de contraste, la similitud entre dos entidades A y B depende no s{\'o}lo de las caracter{\'i}sticas comunes entre las entidades sino tambi{\'e}n de las caracter{\'i}sticas {\'u}nicas. Si A y B son dos entidades descritas mediante atributos booleanos, la similitud entre A y B se define como Sim(A,B) = af(AB)-bf(A-B)-cf(B-A) donde, AB es el conjunto de caracter{\'i}sticas presentes tanto en A como en B, A-B es el conjunto de caracter{\'i}sticas presentes en A pero no en B y B-A es el conjunto de caracter{\'i}sticas presentes en B pero no en A. f es una funci{\'o}n mon{\'o}tona creciente y a, b y c son pesos que pueden establecerse para reforzar la comunalidad o la diferencias en las caracter{\'i}sticas de A y B. Puesto que b y c pueden tener pesos diferentes, la medida resultante puede no ser sim{\'e}trica.",
	added-at = "2009-07-15T12:20:01.000+0200",
	author = "Tversky, Amos",
	biburl = "http://www.bibsonomy.org/bibtex/27504fb00604c13b843220ede288aac73/asalber",
	interhash = "03a061a2e7ecca2b5e8d900655596144",
	intrahash = "7504fb00604c13b843220ede288aac73",
	journal = "Psychological Review",
	keywords = "similarity",
	month = "July",
	number = 4,
	pages = "327--352",
	timestamp = "2012-06-07T11:09:06.000+0200",
	title = "{Features of Similarity}",
	url = "http://dx.doi.org/10.1037/0033-295X.84.4.327",
	volume = 84,
	year = 1977
}

