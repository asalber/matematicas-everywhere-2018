@article{song2006fuzzy,
	abstract = "Se presenta una medida de similitud difusa entre conceptos de una ontolog{\'i}a, basada en la representaci{\'o}n de cada concepto como un conjunto difuso sobre el grafo de relaciones de la ontolog{\'i}a. La mayor parte de las medidas de similitud pertenecen a dos grandes enfoques: - Similitudes basadas en distancias en una red o grafo. Miden la similitud por medio de los caminos que unen los conceptos en la taxonom{\'i}a o estructura de relaciones de la ontolog{\'i}a. - Similitudes basadas en contenido de la informaci{\'o}n. La similitud entre conceptos depende de la informaci{\'o}n que comparten. Utilizan la teor{\'i}a de la informaci{\'o}n y calculan la probabilidad de cada concepto por medio de sus instancias. La medida de similitud difusa que se presenta se construye a partir del modelo de Tversky integrando la informaci{\'o}n que aportan los caminos en la taxonom{\'i}a y el contenido de informaci{\'o}n de los conceptos. La medida trata de recoger las siguientes premisas premisas: - La similitud no debe ser sim{\'e}trica: Es mayor el parecido de un concepto hijo a uno padre en la taxonom{\'i}a (generalizaci{\'o}n), que el de un padre a un hijo (especializaci{\'o}n). - Las similud de conceptos hermanos en los niveles altos de la taxonom{\'i}a (conceptos m{\'a}s generales) debe ser menor que en niveles bajos (conceptos m{\'a}s espec{\'i}ficos). - La similitud de hermanos en una familia con alta densidad de hermanos debe ser mayor que la de hermanos en una familia con baja densidad. Cada concepto se transforma en un conjunto difuso formado por todos los conceptos que se encuentran en el camino a la ra{\'i}z de la taxonom{\'i}a. El grado de pertenencia de cada concepto al conjunto viene dado por los pesos que se asocian a las aristas y que dependen del sentido y del grado de ramificaci{\'o}n del padre. La medida de similitud puede utilizar cualquier tipo de relaci{\'o}n, no s{\'o}lo taxon{\'o}micas, entre conceptos, como por ejemplo, relaciones causa-efecto.",
	added-at = "2009-07-20T16:09:01.000+0200",
	address = "Los Alamitos, CA, USA",
	author = "Song, Ling and Ma, Jun and Lian, Li and Chen, Zhumin",
	biburl = "http://www.bibsonomy.org/bibtex/2ff675d326ebb8b589d73fe5c04f70b21/asalber",
	description = "Fuzzy Similarity from Conceptual Relations",
	interhash = "e49ee68a74abdf3a5fb2b199b12f7f8d",
	intrahash = "ff675d326ebb8b589d73fe5c04f70b21",
	journal = "Asia-Pacific Conference on Services Computing. 2006 IEEE",
	keywords = "1 fuzzy ontologies similarity",
	pages = "3--10",
	publisher = "IEEE Computer Society",
	timestamp = "2012-06-07T11:09:06.000+0200",
	title = "{Fuzzy Similarity from Conceptual Relations}",
	url = "http://www2.computer.org/portal/web/csdl/doi/10.1109/APSCC.2006.58",
	volume = 0,
	year = 2006
}

@article{shvaiko2005survey,
	abstract = "Se hace una clasificaci{\'o}n de las t{\'e}cnicas de emparejamiento de esquemas y ontolog{\'i}as, basadas en la propia informaci{\'o}n contenida en el esquema (no se consideran las intancias) y una revisi{\'o}n de los {\'u}ltimos sistemas de detecci{\'o}n de correspondencias de acuerdo a dicha clasificaci{\'o}n. La clasificaci{\'o}n se realiza en tres dimensiones: - Seg{\'u}n el tipo de informaci{\'o}n de entrada que utilizan los algoritmos de emparejamiento (elementos, esquema, instancias). - Seg{\'u}n la interpretaci{\'o}n que se hace de la informaci{\'o}n de entrada (sint{\'a}ctica, sem{\'a}ntica) y la naturaleza del proceso de emparejamiento (aproximada o exacta). - Seg{\'u}n el tipo de correspondencias que producen (biyectivas o no, con confianza o no, tipo de relaci{\'o}n: equivalencia, subsunci{\'o}n, disyunci{\'o}n, incompatibilidad) Categor{\'i}as: - Nivel de elemento. Analizan las entidades de manera aislada -- Interpretaci{\'o}n sint{\'a}ctica. S{\'o}lo tienen en cuenta la sintaxis de los elementos analizados: T{\'e}cnicas basadas en cadenas (prefijos, sufijos, distancia de edici{\'o}n, n-gramas), t{\'e}cnicas ling{\"u}isticas (tokenizaci{\'o}n, lematizaci{\'o}n, eliminaci{\'o}n de palabras habituales, c{\'a}lculo de stems), t{\'e}cnicas basadas en restricciones (comparaci{\'o}n de tipos de datos, multiplicidades) -- Interpretaci{\'o}n externa. Utilizan recursos externos (por ejemplo tesauros) para interpretar los elementos: T{\'e}cnicas ling{\"u}isticas (tesauros de uso general como Wordnet, tesauros de dominio espec{\'i}fico), t{\'e}cnicas de reutilizaci{\'o}n de emparejamientos, utilizaci{\'o}n de ontolog{\'i}as de alto nivel (como SUMO y DOLCE). - Nivel de esquema. Analizan las entidades seg{\'u}n las relaciones estructurales con otras entidades del esquema. -- Interpretaci{\'o}n sint{\'a}ctica: T{\'e}cnicas basadas en grafos (hijos, padres, hojas, caminos en grafos, etc), t{\'e}cnicas taxon{\'o}micas (padres, hijos, ancestros). -- Interpretaci{\'o}n sem{\'a}ntica. Utilizan alguna l{\'o}gica formal para interpretar los elementos. T{\'e}cnicas basadas en modelos l{\'o}gicos formales (Satisfactibilidad proposicional, razonadores con l{\'o}gicas descriptivas). Sistemas revisados: Similarity Flooding (SF), Artemis, Cupid, COMA, Naive Ontology Mapping (NOM), Quick Ontology Mapping (QOM), OWL Lite Aligner (OLA), Anchor-Prompt, S-Match.",
	added-at = "2009-07-20T15:41:52.000+0200",
	author = "Shvaiko, Pavel and Euzenat, J{\'e}r{\^o}me",
	biburl = "http://www.bibsonomy.org/bibtex/2dd190e6fd7f9cf97ec8fb32e8974f205/asalber",
	interhash = "759d45f827c4f0f64a534b08d6819571",
	intrahash = "dd190e6fd7f9cf97ec8fb32e8974f205",
	journal = "Journal on Data Semantics IV",
	keywords = "2 matching ontologies state-of-art ontology-alignment",
	pages = "146--171",
	publisher = "Springer",
	timestamp = "2012-06-07T11:07:57.000+0200",
	title = "{A Survey of Schema-Based Matching Approaches}",
	volume = 3730,
	year = 2005
}

@inproceedings{shvaiko2008ten,
	abstract = "Se analiza el estado del arte en la correspondencia de ontolog{\'i}as y se apuntan los diez retos m{\'a}s importantes para la futura investigaci{\'o}n en este campo: - Necesidad de t{\'e}cnicas de evaluaci{\'o}n de los sistemas de de correspondencias a gran escala y orientadas a la aplicaci{\'o}n en determinados dominios. - Mejora y optimizaci{\'o}n de la eficiencia de los algoritmos de emparejamiento. - Descubrimiento del conocimiento de base utilizado en el dise{\~n}o de la ontolog{\'i}a pero que no aparece expl{\'i}citamente en la especificaci{\'o}n de la ontolog{\'i}a. Este conocimiento ayudar{\'i}a mucho a los algoritmos de emparejamiento. - Formalizaci{\'o}n y manejo de de la incertidumbre y la informaci{\'o}n incompleta en los sistemas de detecci{\'o}n de correspondencias. - Selecci{\'o}n de los algoritmos de emparejamiento mejores para una determinada aplicaci{\'o}n, combinaci{\'o}n y autoconfiguraci{\'o}n de los mismos. - Iteracci{\'o}n con el usuario e incorporaci{\'o}n de su conocimiento en le proceso de emparejamiento de las ontolog{\'i}as. - Explicaci{\'o}n de los emparejamientos determinados de manera autom{\'a}tica, para hacerlos comprensible para los humanos. - Aprovechar el trabajo colaborativo en red para mejorar la construcci{\'o}n de correspondencias. - Dar soporte a todas las tareas que requiere el ciclo de vida de las correspondencias entre ontolog{\'i}as. - Razonamiento con las correspondencias entre ontolog{\'i}as.",
	added-at = "2009-07-20T15:41:05.000+0200",
	author = "Shvaiko, Pavel and Euzenat, J{\'e}r{\^o}me",
	biburl = "http://www.bibsonomy.org/bibtex/27719dca151da48d6775f8cf29cebbfc2/asalber",
