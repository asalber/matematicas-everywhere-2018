	intrahash = "f3d86603190f4bb3ab11793b9159f88a",
	journal = "IEEE Transactions on Knowledge and Data Engineering",
	keywords = "no-leido ontologies similarity",
	number = 2,
	pages = "442--456",
	timestamp = "2012-06-07T11:09:06.000+0200",
